       {
          "id":6967691037749466,
          "definition":{
             "title":"Arrays",
             "title_size":"16",
             "title_align":"left",
             "type":"query_table",
             "requests":[
                {
                   "formulas":[
                      {
                         "alias":"Info",
                         "conditional_formats":[
                            {
                               "palette":"black_on_light_yellow",
                               "comparator":">=",
                               "value":1
                            }
                         ],
                         "cell_display_mode":"number",
                         "formula":"query1"
                      },
                      {
                         "alias":"Warning",
                         "conditional_formats":[
                            {
                               "palette":"white_on_yellow",
                               "comparator":">=",
                               "value":1
                            }
                         ],
                         "cell_display_mode":"number",
                         "formula":"query2"
                      },
                      {
                         "alias":"Critical",
                         "conditional_formats":[
                            {
                               "palette":"white_on_red",
                               "comparator":">=",
                               "value":1
                            }
                         ],
                         "limit":{
                            "count":500,
                            "order":"desc"
                         },
                         "cell_display_mode":"number",
                         "formula":"query3"
                      },
                      {
                         "alias":"Total Alerts",
                         "conditional_formats":[
                            
                         ],
                         "cell_display_mode":"number",
                         "formula":"query4"
                      },
                      {
                         "alias":"Hardware Health",
                         "conditional_formats":[
                            {
                               "palette":"white_on_red",
                               "comparator":"<",
                               "value":1
                            },
                            {
                               "palette":"white_on_green",
                               "comparator":">=",
                               "value":1
                            }
                         ],
                         "cell_display_mode":"number",
                         "formula":"query5"
                      }
                   ],
                   "response_format":"scalar",
                   "queries":[
                      {
                         "query":"sum:purefb.alerts.open{$ArrayName,severity:info} by {fb_array_name}",
                         "data_source":"metrics",
                         "name":"query1",
                         "aggregator":"last"
                      },
                      {
                         "query":"sum:purefb.alerts.open{$ArrayName,severity:warning} by {fb_array_name}",
                         "data_source":"metrics",
                         "name":"query2",
                         "aggregator":"last"
                      },
                      {
                         "query":"sum:purefb.alerts.open{$ArrayName,severity:critical} by {fb_array_name}",
                         "data_source":"metrics",
                         "name":"query3",
                         "aggregator":"last"
                      },
                      {
                         "query":"sum:purefb.alerts.open{$ArrayName,severity:*} by {fb_array_name}",
                         "data_source":"metrics",
                         "name":"query4",
                         "aggregator":"last"
                      },
                      {
                         "query":"min:purefb.hardware.health{$ArrayName} by {fb_array_name}",
                         "data_source":"metrics",
                         "name":"query5",
                         "aggregator":"last"
                      }
                   ]
                }
             ],
             "has_search_bar":"auto"
          },
          "layout":{
             "x":2,
             "y":0,
             "width":10,
             "height":2
          }
       },
